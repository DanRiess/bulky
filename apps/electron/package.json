{
	"name": "bulky",
	"version": "0.0.1",
	"description": "Bulk trading in-game overlay for Path of Exile. Made with Electron, VueJs and Typescript.",
	"main": "./out/main/index.js",
	"author": "Daniel Riess",
	"homepage": "",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "pnpm typecheck:node && pnpm typecheck:web",
		"start": "electron-vite preview",
		"dev": "electron-vite dev --watch",
		"build": "npm run typecheck && electron-vite build",
		"build:win": "pnpm build && electron-builder --win --config",
		"build:mac": "pnpm build && electron-builder --mac --config",
		"build:linux": "pnpm build && electron-builder --linux --config"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^3.0.0",
		"@electron-toolkit/utils": "^3.0.0",
		"@vueuse/core": "^10.7.2",
		"active-win": "^8.2.1",
		"axios": "^1.6.7",
		"electron-overlay-window": "^3.3.0",
		"electron-updater": "^6.1.7",
		"express": "^4.19.2",
		"gsap": "^3.12.5",
		"uiohook-napi": "^1.5.3",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@dan_riess/eslint-config": "^0.0.1",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/express": "^4.17.21",
		"@types/lodash": "^4.14.202",
		"@types/node": "^18.19.5",
		"@types/uuid": "^9.0.8",
		"@vitejs/plugin-vue": "^5.0.2",
		"electron": "^28.1.1",
		"electron-builder": "^24.9.1",
		"electron-vite": "^2.0.0",
		"lodash": "^4.17.21",
		"pinia": "^2.1.7",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vue": "^3.4.5",
		"vue-router": "^4.2.5",
		"vue-tsc": "^1.8.27"
	}
}